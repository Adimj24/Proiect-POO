#include <iostream>

using namespace std;

class IntSet{ // asta am de luat de aici
private:

   int *v,nr=0,indice=0;

public:

    IntSet(int dim)
    {
        nr=dim;
        v = new int[dim];
    }

    bool contains(int);
    IntSet stergere(int);
    IntSet add(int);
    int lungime();
    void afisare();

};
void IntSet:: afisare() // si asta
{
    int i;
    cout<<'\n'<<"Multimea: ";

    for (i=0 ; i<indice ; i++)
        cout<<v[i]<<" ";

    cout<<'\n';
}

int IntSet:: lungime() // si asta de asemenea
{
  return indice;
}
